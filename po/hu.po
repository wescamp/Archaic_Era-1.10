#: Archaic_Era/units/khthon/Terrapin.cfg:5
msgid "Terrapin"
msgstr "Földhözragadt"

#. [unit_type]: id=Terrapin, race=khthon
#. [unit_type]: id=Terrapin_Hiding, race=khthon
#: Archaic_Era/units/khthon/Terrapin.cfg:20
#: Archaic_Era/units/khthon/Terrapin.cfg:115
